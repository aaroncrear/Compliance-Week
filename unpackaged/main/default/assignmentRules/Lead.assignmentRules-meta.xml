<?xml version="1.0" encoding="utf-8"?><AssignmentRules xmlns="http://soap.sforce.com/2006/04/metadata">
    <assignmentRule>
        <fullName>Wilmington Group</fullName>
        <active>true</active>
        <ruleEntry>
            <assignedTo>CW_Ad_Sales_Lead_Queue</assignedTo>
            <assignedToType>Queue</assignedToType>
            <criteriaItems>
                <field>Lead.RecordTypeId</field>
                <operation>equals</operation>
                <value>Advertising</value>
            </criteriaItems>
            <criteriaItems>
                <field>Lead.Business_Unit__c</field>
                <operation>equals</operation>
                <value>Compliance Week</value>
            </criteriaItems>
            <criteriaItems>
                <field>Lead.Status</field>
                <operation>equals</operation>
                <value>New</value>
            </criteriaItems>
            <template>SubSalesEmails/New_Lead_Assignment</template>
        </ruleEntry>
        <ruleEntry>
            <assignedTo>Subscription_Lead_Queue</assignedTo>
            <assignedToType>Queue</assignedToType>
            <booleanFilter>1 AND 2 AND 3 AND 4 AND 5</booleanFilter>
            <criteriaItems>
                <field>Lead.RecordTypeId</field>
                <operation>equals</operation>
                <value>Subscriptions</value>
            </criteriaItems>
            <criteriaItems>
                <field>Lead.Business_Unit__c</field>
                <operation>equals</operation>
                <value>Compliance Week</value>
            </criteriaItems>
            <criteriaItems>
                <field>Lead.CreatedDate</field>
                <operation>equals</operation>
                <value>TODAY</value>
            </criteriaItems>
            <criteriaItems>
                <field>Lead.Status</field>
                <operation>equals</operation>
                <value>New</value>
            </criteriaItems>
            <criteriaItems>
                <field>Lead.LeadSource</field>
                <operation>notEqual</operation>
                <value>List Acquisition</value>
            </criteriaItems>
        </ruleEntry>
        <ruleEntry>
            <assignedTo>Marketing_Queue</assignedTo>
            <assignedToType>Queue</assignedToType>
            <criteriaItems>
                <field>Lead.RecordTypeId</field>
                <operation>equals</operation>
                <value>Subscriptions</value>
            </criteriaItems>
            <criteriaItems>
                <field>Lead.Business_Unit__c</field>
                <operation>equals</operation>
                <value>Compliance Week</value>
            </criteriaItems>
            <criteriaItems>
                <field>Lead.CreatedDate</field>
                <operation>equals</operation>
                <value>TODAY</value>
            </criteriaItems>
            <criteriaItems>
                <field>Lead.Status</field>
                <operation>equals</operation>
                <value>New</value>
            </criteriaItems>
        </ruleEntry>
    </assignmentRule>
</AssignmentRules>
