    <rules>
        <fullName>Closed Won via Renewal Email</fullName>
        <actions>
            <name>Notify_Opportunity_owner_that_Opp_has_been_Closed_Won_via_renewal_email</name>
            <type>Alert</type>
        </actions>
        <active>false</active>
        <criteriaItems>
            <field>Opportunity.Closed_Won_Via_Email__c</field>
            <operation>equals</operation>
            <value>True</value>
        </criteriaItems>
        <description>Triggers an email notification Angela Ruggiero when a customer chooses to renew their subscription via 'Accept' email button.</description>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
    <rules>
        <fullName>Multi-User Closed Won</fullName>
        <actions>
            <name>Tell_Angela_About_Multi</name>
            <type>Alert</type>
        </actions>
        <active>true</active>
        <criteriaItems>
            <field>Opportunity.RecordTypeId</field>
            <operation>equals</operation>
            <value>Subscription Opportunity</value>
        </criteriaItems>
        <criteriaItems>
            <field>Opportunity.Type</field>
            <operation>equals</operation>
            <value>Subscription</value>
        </criteriaItems>
        <criteriaItems>
            <field>Opportunity.Single_Multi__c</field>
            <operation>equals</operation>
            <value>Multi</value>
        </criteriaItems>
        <criteriaItems>
            <field>Opportunity.StageName</field>
            <operation>equals</operation>
            <value>Closed Won</value>
        </criteriaItems>
        <criteriaItems>
            <field>User.ProfileId</field>
            <operation>notEqual</operation>
            <value>System Administrator</value>
        </criteriaItems>
        <description>Alert Angela when multi-user subscription is sold.</description>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
    <rules>
        <fullName>Opportunity Business Unit</fullName>
        <actions>
            <name>Update_Business_Unit</name>
            <type>FieldUpdate</type>
        </actions>
        <active>false</active>
        <description>Tags Opportunity with the Business Unit of the user creating the record.</description>
        <formula>ISPICKVAL($User.Business_Unit__c, "Compliance Week")</formula>
        <triggerType>onCreateOnly</triggerType>
    </rules>
    <rules>
        <fullName>Recapture Opportunity Closed Lost 90 days after created</fullName>
        <active>false</active>
        <description>Recapture Opportunities are Closed Lost 90 days after created. Lost Reason = Recapture Opportunity Expired.</description>
        <formula>AND( ISPICKVAL(Type,"Subscription"), ISPICKVAL(Business_Type__c,"Recacpture"), IsClosed = FALSE, Business_Unit__c = "Compliance Week" )</formula>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
        <workflowTimeTriggers>
            <actions>
                <name>Lost_Reason_Recature_Opportunity_Expir</name>
                <type>FieldUpdate</type>
            </actions>
            <actions>
